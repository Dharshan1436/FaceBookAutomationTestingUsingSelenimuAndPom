<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FaceBook Application Automation Test And Test Suites ">
    <listeners>
        <listener class-name="extentreportlistener.ExtentReporterNG" />
    </listeners>

    <test name="Facebook application Testcases">

        <classes>
            <class name="com.fb.qa.testcases.LoginPageTest"/>
            <class name="com.fb.qa.testcases.CreateNewAccountPageTest"/>
            <class name="com.fb.qa.testcases.HomePageTest"/>
            <class name="com.fb.qa.testcases.ProfilePageTest"/>
            <class name="com.fb.qa.testcases.FriendsPageTest"/>
        </classes>
    </test>
</suite>